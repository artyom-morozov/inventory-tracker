<p style="color: green"><%= notice %></p>

<h1>Items</h1>
<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Title</th>
      <th scope="col">Description</th>
      <th scope="col">Price</th>
      <th scope="col">Inventory Count</th>
      <th scope="col">Actions</th>
    </tr>
  </thead>
  <tbody>
   <% @items.each do |item| %>
      <%= render item %>
    <% end %>
    
  </tbody>
</table>

